pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                sh ''' echo "this is a static website" '''
            }
        }
        stage('Test') {
            steps {
                sh ''' echo "this is static website test phase" '''
            }
        }
        
    }

    post {
        success {
            echo 'Build successful!'
        }
        failure {
            echo 'Build failed.'
        }
    }
}
